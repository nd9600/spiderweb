{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --mode=development",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --mode=development",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --mode=production",
    "test": "jest resources/assets/js/__tests__"
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "last 2 Firefox major versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@types/d3": "^5.7.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^5.1",
    "css-loader": "^3.6.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-cssimport": "^7.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "vinyl-named-with-path": "^1.0.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "d3": "^5.16.0",
    "firebase": "^7.19.0",
    "lodash.debounce": "^4.0.8",
    "marked": "^1.1.1",
    "vue": "^2.6.12",
    "vuex": "^3.5.1"
  },
  "jest": {
    "bail": false,
    "verbose": false,
    "globals": {
      "NODE_ENV": "test"
    },
    "testRegex": "(/__tests__/.*test)\\.js$",
    "moduleFileExtensions": ["js", "json", "ts"],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/resources/assets/js/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "-ignore"
    ]
  }
}
